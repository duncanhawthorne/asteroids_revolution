{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
  },
    "headers": [
  {
    "regex": "/.*",
    "headers": [
      {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "credentialless"
      },
      {
        "key": "Cross-Origin-Opener-Policy",
        "value": "same-origin"
      }
    ]
  }
  ]
}
